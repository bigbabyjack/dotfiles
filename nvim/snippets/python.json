{
  "Function with type hints": {
    "prefix": "deft",
    "body": [
      "def ${1:function_name}(${2:arg}: ${3:str}) -> ${4:None}:",
      "    \"\"\"${5:Description}",
      "",
      "    Args:",
      "        ${2:arg} (${3:str}): ${6:Description}",
      "",
      "    Returns:",
      "        ${4:None}: ${7:Description}",
      "    \"\"\"",
      "    ${0:pass}"
    ],
    "description": "Function with type hints and docstring"
  },

  "Async function with type hints": {
    "prefix": "adef",
    "body": [
      "async def ${1:function_name}(${2:arg}: ${3:str}) -> ${4:None}:",
      "    \"\"\"${5:Description}\"\"\"",
      "    ${0:pass}"
    ],
    "description": "Async function with type hints"
  },

  "Class with init": {
    "prefix": "classi",
    "body": [
      "class ${1:ClassName}:",
      "    \"\"\"${2:Description}\"\"\"",
      "",
      "    def __init__(self, ${3:arg}: ${4:str}) -> None:",
      "        \"\"\"Initialize ${1:ClassName}.",
      "",
      "        Args:",
      "            ${3:arg} (${4:str}): ${5:Description}",
      "        \"\"\"",
      "        self.${3:arg} = ${3:arg}",
      "        ${0}",
      ""
    ],
    "description": "Class with __init__ method"
  },

  "Dataclass": {
    "prefix": "dataclass",
    "body": [
      "from dataclasses import dataclass",
      "",
      "@dataclass",
      "class ${1:ClassName}:",
      "    \"\"\"${2:Description}\"\"\"",
      "    ${3:field}: ${4:str}",
      "    ${0}"
    ],
    "description": "Python dataclass"
  },

  "Try except with logging": {
    "prefix": "trylog",
    "body": [
      "try:",
      "    ${1:pass}",
      "except ${2:Exception} as e:",
      "    ${3:logger}.error(f\"${4:Error}: {e}\")",
      "    ${0:raise}"
    ],
    "description": "Try/except with logging"
  },

  "Context manager": {
    "prefix": "with",
    "body": [
      "with ${1:open}(${2:file}) as ${3:f}:",
      "    ${0:pass}"
    ],
    "description": "Context manager (with statement)"
  },

  "Main guard": {
    "prefix": "main",
    "body": [
      "def main() -> None:",
      "    \"\"\"${1:Main function}\"\"\"",
      "    ${2:pass}",
      "",
      "",
      "if __name__ == \"__main__\":",
      "    main()"
    ],
    "description": "Main function with guard"
  },

  "Pytest fixture": {
    "prefix": "fixture",
    "body": [
      "@pytest.fixture",
      "def ${1:fixture_name}():",
      "    \"\"\"${2:Description}\"\"\"",
      "    ${3:value} = ${4:None}",
      "    yield ${3:value}",
      "    ${0:# Cleanup}"
    ],
    "description": "Pytest fixture"
  },

  "Pytest parametrize": {
    "prefix": "parametrize",
    "body": [
      "@pytest.mark.parametrize(",
      "    \"${1:arg1},${2:arg2},${3:expected}\",",
      "    [",
      "        (${4:value1}, ${5:value2}, ${6:result}),",
      "        ${0}",
      "    ],",
      ")",
      "def test_${7:function_name}(${1:arg1}, ${2:arg2}, ${3:expected}):",
      "    assert ${8:function}(${1:arg1}, ${2:arg2}) == ${3:expected}"
    ],
    "description": "Pytest parametrize decorator"
  },

  "List comprehension": {
    "prefix": "lc",
    "body": [
      "[${1:x} for ${1:x} in ${2:iterable} if ${3:condition}]$0"
    ],
    "description": "List comprehension"
  },

  "Dictionary comprehension": {
    "prefix": "dc",
    "body": [
      "{${1:key}: ${2:value} for ${1:key}, ${2:value} in ${3:iterable} if ${4:condition}}$0"
    ],
    "description": "Dictionary comprehension"
  },

  "Type alias": {
    "prefix": "typealias",
    "body": [
      "${1:AliasName} = ${2:dict}[${3:str}, ${4:Any}]$0"
    ],
    "description": "Type alias definition"
  },

  "FastAPI route": {
    "prefix": "fastapi",
    "body": [
      "@app.${1:get}(\"/${2:path}\")",
      "async def ${3:function_name}(${4:arg}: ${5:str}) -> ${6:dict}:",
      "    \"\"\"${7:Description}\"\"\"",
      "    ${0:return {\"status\": \"ok\"}}"
    ],
    "description": "FastAPI route handler"
  },

  "Pydantic model": {
    "prefix": "pydantic",
    "body": [
      "class ${1:ModelName}(BaseModel):",
      "    \"\"\"${2:Description}\"\"\"",
      "    ${3:field}: ${4:str}",
      "    ${0}"
    ],
    "description": "Pydantic model"
  },

  "Logger setup": {
    "prefix": "logger",
    "body": [
      "import logging",
      "",
      "logger = logging.getLogger(__name__)",
      "${0}"
    ],
    "description": "Setup module logger"
  },

  "Enum class": {
    "prefix": "enum",
    "body": [
      "from enum import Enum",
      "",
      "class ${1:EnumName}(Enum):",
      "    \"\"\"${2:Description}\"\"\"",
      "    ${3:OPTION1} = \"${4:value1}\"",
      "    ${5:OPTION2} = \"${6:value2}\"",
      "    ${0}"
    ],
    "description": "Enum class"
  },

  "Property decorator": {
    "prefix": "property",
    "body": [
      "@property",
      "def ${1:property_name}(self) -> ${2:str}:",
      "    \"\"\"${3:Description}\"\"\"",
      "    return self._${1:property_name}",
      "",
      "@${1:property_name}.setter",
      "def ${1:property_name}(self, value: ${2:str}) -> None:",
      "    self._${1:property_name} = value",
      "    ${0}"
    ],
    "description": "Property with getter and setter"
  }
}
